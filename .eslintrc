{
    "root": true,
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "amd": true
    },
    "rules": {
      "prefer-promise-reject-errors": 0,
      "react/prop-types": 0,
      "react/no-deprecated": 0,
      "prettier/prettier": [
        2,
        {
          "singleQuote": true
        }
      ],
      "no-useless-constructor": 0,
      "react/no-string-refs": "off",
      "no-debugger": "off"
    },
    "extends": [
      "standard",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ]
}